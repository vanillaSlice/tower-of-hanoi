{
  "name": "tower-of-hanoi",
  "version": "1.0.0",
  "description": "Tower of Hanoi puzzle written to practice using Babel and Webpack",
  "author": "Mike Lowe",
  "private": true,
  "dependencies": {
    "papercss": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "autoprefixer": "^9.4.5",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "stylelint": "^9.9.0",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "stylelint-order": "^2.0.0",
    "stylelint-scss": "^3.4.4",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.2.1"
  },
  "homepage": "https://vanillaslice.github.io/tower-of-hanoi/",
  "scripts": {
    "lint": "run-s lint:*",
    "lint:js": "eslint src/js",
    "lint:scss": "stylelint src/scss",
    "prebuild": "run-s lint",
    "build": "run-s build:*",
    "build:css": "node-sass src/scss -o src/css",
    "build:webpack": "webpack --config webpack.prod.js",
    "watch:scss": "node-sass src/scss -o src/css -w",
    "start": "run-p watch:scss start:*",
    "start:webpack": "webpack-dev-server --open --config webpack.dev.js",
    "predeploy": "run-s build",
    "deploy": "gh-pages -d dist"
  }
}
