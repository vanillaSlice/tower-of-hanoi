{
  "name": "tower-of-hanoi",
  "version": "1.1.0",
  "description": "Tower of Hanoi puzzle written to practice using Babel and Webpack",
  "keywords": [
    "tower of hanoi",
    "babel",
    "webpack"
  ],
  "homepage": "https://towerofhanoi.mikelowe.xyz/",
  "bugs": {
    "url": "https://github.com/vanillaSlice/tower-of-hanoi/issues",
    "email": "mikelowedev@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Mike Lowe",
    "email": "mikelowedev@gmail.com",
    "url": "https://www.mikelowe.xyz/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vanillaSlice/tower-of-hanoi.git"
  },
  "scripts": {
    "pretest": "run-s build:css",
    "test": "run-s test:*",
    "test:lint": "run-s test:lint:*",
    "test:lint:scss": "stylelint src/**/*.scss",
    "test:lint:js": "eslint src/**/*.js",
    "start": "run-p start:*",
    "start:scss": "node-sass src/styles -o src/styles -w",
    "start:webpack": "webpack-dev-server --open --config webpack.dev.js",
    "build": "run-s build:*",
    "build:css": "node-sass src/styles -o src/styles",
    "build:webpack": "webpack --config webpack.prod.js"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.9.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "private": true
}
