{
  "name": "tower-of-hanoi",
  "version": "1.0.0",
  "description": "Tower of Hanoi puzzle written to practice using Babel and Webpack",
  "keywords": [
    "tower of hanoi",
    "babel",
    "webpack"
  ],
  "homepage": "https://github.com/vanillaSlice/tower-of-hanoi#readme",
  "bugs": {
    "url": "https://github.com/vanillaSlice/tower-of-hanoi/issues",
    "email": "mikelowedev@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Mike Lowe",
    "email": "mikelowedev@gmail.com",
    "url": "https://www.mikelowe.xyz/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vanillaSlice/tower-of-hanoi.git"
  },
  "scripts": {
    "pretest": "run-s build:css",
    "test": "run-s test:*",
    "test:lint": "run-s test:lint:*",
    "test:lint:scss": "stylelint src/**/*.scss",
    "test:lint:js": "eslint src/**/*.js",
    "start": "run-p start:*",
    "start:scss": "node-sass src/styles -o src/styles -w",
    "start:webpack": "webpack-dev-server --open --config webpack.dev.js",
    "build": "run-s build:*",
    "build:css": "node-sass src/styles -o src/styles",
    "build:webpack": "webpack --config webpack.prod.js"
  },
  "dependencies": {
    "papercss": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^9.4.7",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "private": true
}
